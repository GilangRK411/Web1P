os from 'axios'\r\nexport default {\r\n    data() {\r\n        return {\r\n            username: '',\r\n            password: '',\r\n        };\r\n    },\r\n    methods:{\r\n        login:function(){\r\n            axios.post(\"http://localhost:3000\",{\r\n                username: this.username,\r\n                password: this.password\r\n            }).then((res)=>{\r\n                alert(\"berhasil login\");\r\n                console.log(res);\r\n            }).catch((error) => {\r\n                alert(\"Gagal login\");\r\n                console.error(error);\r\n            });\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n* {\r\n  box-sizing: border-box;\r\n}\r\n.logincontainer {\r\n  left: 0px;\r\n}\r\n.containerbox1 {\r\n  width: 1920px;\r\n  height: 1000px;\r\n  background: rgba(58,58,58,1);\r\n  opacity: 1;\r\n  overflow: hidden;\r\n  position: absolute;\r\n  right: 950px;\r\n  z-index: -1;\r\n  border-right: 5px solid black;\r\n\r\n}\r\n.login {\r\n  width: 400px;\r\n  color: rgb(0, 0, 0);\r\n  position: absolute;\r\n  top: 238px;\r\n  right: 250px;\r\n  font-family: Montserrat;\r\n  font-weight: Thin Italic;\r\n  font-size: 96px;\r\n  opacity: 1;\r\n  text-align: left;\r\n}\r\ninput::placeholder {\r\n  color: rgb(0, 0, 0);\r\n}\r\n.passwordbox {\r\n  width: 138px;\r\n  position: absolute;\r\n  top: 492px;\r\n  right: 620px;\r\n}\r\n.password {\r\n  position: absolute;\r\n  font-family: Montserrat;\r\n  font-weight: Regular;\r\n  font-size: 20px;\r\n  opacity: 1;\r\n  text-align: left;\r\n  padding: 10px;\r\n  border: none;\r\n  outline: none;\r\n}\r\n.usernamebox {\r\n  width: 138px;\r\n  position: absolute;\r\n  top: 396px;\r\n  right: 620px;\r\n  \r\n}\r\n.username {\r\n  position: absolute;\r\n  font-family: Montserrat;\r\n  font-weight: Regular;\r\n  font-size: 20px;\r\n  opacity: 1;\r\n  text-align: left;\r\n  border: none;\r\n  padding: 10px;\r\n  outline: none;\r\n}\r\n.picture1 {\r\n  width: 981px;\r\n  heig