 {\r\n              console.log(data.status);\r\n              try {\r\n                if (data.status === true) {\r\n                  this.showSuccessMessage('Login Successful');\r\n                  setTimeout(() => {\r\n                    this.$router.push('/page1');\r\n                  }, 1000);\r\n                } else {\r\n                  this.showError('Incorrect email or password');\r\n                }\r\n              } catch (error) {\r\n                this.showError('Error, please try again');\r\n              }\r\n            })\r\n            .catch((error) => {\r\n              this.showError('An error occurred. Please try again later.');\r\n            })\r\n            .finally(() => {\r\n              this.isLoading = false; // Set loading state to false\r\n            });\r\n        } else {\r\n          this.showError('Please enter an email');\r\n        }\r\n      },\r\n      showSuccessMessage(message) {\r\n        this.$toast.success(message); // Display a success message using a toast notification library or method of your choice\r\n      },\r\n      showError(message) {\r\n        this.loginError = message; // Set the error message\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n* {\r\n  box-sizing: border-box;\r\n}\r\n.logincontainer {\r\n  left: 0px;\r\n}\r\n.containerbox1 {\r\n  width: 1920px;\r\n  height: 1000px;\r\n  background: rgba(58,58,58,1);\r\n  opacity: 1;\r\n  overflow: hidden;\r\n  position: absolute;\r\n  right: 950px;\r\n  z-index: -1;\r\n  border-right: 5px solid black;\r\n\r\n}\r\n.login {\r\n  width: 400px;\r\n  color: rgb(0, 0, 0);\r\n  position: absolute;\r\n  top: 238px;\r\n  right: 250px;\r\n  font-family: Montserrat;\r\n  font-weight: Thin Italic;\r\n  font-size: 96px;\r\n  opacity: 1;\r\n  text-align: left;\r\n}\r\ninput::placeholder {\r\n  color: rgb(0, 0, 0);\r\n}\r\n.passwordbox {\r\n  width: 138px;\r\n  position: absolute;\r\n  top: 492px;\r\n  right: 620px;\r\n}\r\n.pas